<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.Paginas.MarcarConsultas"
             Title="Login">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout Margin="10">
                <Label Text="Digite seu CPF:" FontAttributes="Bold" FontSize="Large"/>
                <Entry x:Name="Cpf" Placeholder="123.456.789-10"/>
                <Button Text="ENTRAR" Clicked="Entrar"/>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>